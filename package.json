{
  "name": "scripts",
  "version": "0.0.1",
  "description": "Create a Node.js cross exchange daemon for perform user cross exchange analisis",
  "bin": "index.js",
  "main": "index.js",
  "repository": "https://github.com/cryptween/scripts.git",
  "author": "israel gonzalez medina <israel.gonzalezmedina@gmail.com>",
  "license": "MIT",
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "build:exec": "npx pkg -t linux,macos,win createDaemonExecutables.js --out-path dist",
    "installer:linux": "npx pkg -t linux,macos,win . --out-path dist"
  },
  "keywords": [
    "node",
    "node.js",
    "es6",
    "es7",
    "es8",
    "es9",
    "docker",
    "joi",
    "eslint"
  ],
  "pkg": {
    "scripts": "src/index.js",
    "assets": "contracts/*",
    "outputPath": "dist"
  }
}
